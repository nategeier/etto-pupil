nav.navbar.navbar-default( role="navigation" )
  .navbar-header
    button.navbar-toggle( data-toggle="collapse", data-target="#etto-navbar" )
      .sr-only Toggle navigation
      .icon-bar
      .icon-bar
      .icon-bar
    a.navbar-brand( href="/" )
      span
        img(src="{{company.logo || '/images/icons/coursetto-144.png'}}", height="50")
      span.logo-name(ng-show="!company || company.logoIsCompanyName === false") {{company.title || 'Coursetto'}}

  #etto-navbar.collapse.navbar-collapse
    .navbar-right

      .etto-navbtn(etto-social-auth, etto-register-modal, ng-click='register()') Register
      .etto-navbtn(etto-social-auth, etto-login-modal, ng-click='login()') Login
