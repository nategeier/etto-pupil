.input-group
  input.form-control(type="text", ng-model="userSearch", placeholder="User full name or email", ng-change="searchUser()")
  .input-group-btn
    button.btn.btn-default
      i.fa.fa-search
div(ng-repeat="user in foundUsers")
  a.btn.btn-default(href="/settings/{{user._id}}") {{user.name}}, {{user.emails[0]}}
        

