.container
  .etto-map
    etto-block-map(ng-init="startMap(block.data)")
    .row
      .col-md-5
        h1.etto.color-primary {{ block.data.title }}  
        p.etto.color-secondary {{ block.data.copy }}

      .col-md-7
        leaflet.etto-map-canvas.etto-img-shadow(bounds="bounds", center="center", markers="markers")
    hr
    .row
      .col-sm-12
        div(etto-sound)

